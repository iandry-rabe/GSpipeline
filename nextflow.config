process {
	//executor = 'awsbatch'
	//queue = 'GSqueue'
	container = 'iandryrabe/gspipeline'
    withName:bwaMapping {
        container = 'd3bcenter/alpine-bwa-samtools'
    }
    withName:addReadGroups {
        container = 'broadinstitute/picard:2.23.3'
    }
	withName:markDups {
        container = 'broadinstitute/gatk:4.1.8.0'
    }
	withName:baseRecal {
        container = 'broadinstitute/gatk:4.1.8.0'
    }
	withName:applyBQSR {
        container = 'broadinstitute/gatk:4.1.8.0'
    }
	withName:haplotypeCaller {
        container = 'broadinstitute/gatk:4.1.8.0'
    }
}

docker {
    enabled = true
}

